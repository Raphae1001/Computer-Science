/**
 * Exclusive-or gate:
 * if ((a and Not(b)) or (Not(a) and b)) out = 1, else out = 0
 */
CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
Nand(a=a, b=b, out=ab);
Nand(a=b, b=a, out=ba);
Nand(a=a, b=ab, out=abbb);
Nand(a=ab, b=b, out=aabbb);
And(a=aabbb, b=abbb, out=aaaabbb);
Not(in=aaaabbb, out=out);


}